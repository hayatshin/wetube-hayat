extends base

block content
    style 
        include ../css/home.css
    div#Container
        h1 Enroll Your Account
        form#UserForm(method="POST" action="/")
            input.Blank(placeholder="Type Your Email", name="email", type="email", required) 
            input.Blank(placeholder="Type Your Password", name="password", type="password", required) 
            input#Btn(type="submit" value="Sign Up")
